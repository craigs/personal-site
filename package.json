{
  "name": "personal-site",
  "version": "1.20.9",
  "description": "This is a personal branding site for Craig Sullivan",
  "author": "Craig Sullivan",
  "private": true,
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "main": "index.js",
  "jest": {
    "setupFiles": [
      "jest-canvas-mock"
    ]
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx --config .eslintrc.json",
    "lint:fix": "eslint . --ext .ts,.tsx --fix --config .eslintrc.json",
    "release": "HUSKY_SKIP_HOOKS=1 standard-version",
    "test": "jest --colors",
    "test:watch": "jest --colors --watch"
  },
  "keywords": [],
  "lint-staged": {
    "*.ts": [
      "npm run lint:fix",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "classnames": "^2.3.1",
    "clsx": "^1.1.1",
    "graphql": "^15.5.1",
    "graphql-tag": "^2.12.5",
    "isomorphic-fetch": "^3.0.0",
    "next": "^11.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-gtm-module": "^2.0.11",
    "react-markdown": "^6.0.2",
    "react-swipeable": "^6.1.2",
    "rehype-raw": "^5.1.0"
  },
  "devDependencies": {
    "@apollo/react-testing": "^4.0.0",
    "@types/classnames": "^2.3.1",
    "@types/express": "^4.17.13",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.3",
    "@types/react": "^17.0.14",
    "@types/react-gtm-module": "^2.0.1",
    "@types/react-test-renderer": "^17.0.1",
    "@types/request": "^2.48.6",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "autoprefixer": "^10.3.1",
    "caniuse-lite": "^1.0.30001245",
    "dotenv": "^10.0.0",
    "dotenv-webpack": "^7.0.3",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.6",
    "jest-canvas-mock": "^2.3.1",
    "lint-staged": "^11.0.1",
    "minimist": "^1.2.5",
    "nock": "^13.1.1",
    "nodemon": "^2.0.12",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.2",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.35.2",
    "standard-version": "^9.3.1",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  }
}
